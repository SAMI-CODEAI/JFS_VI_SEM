<?xml version="1.0"?>
<hibernate-mapping>
	<class name="Course" table="course" dynamic-insert="true" dynamic-update="true">
		<id name="cid" column="cid" length="5">
			<generator class="assigned"/>
		</id>
		<property name="cname" column="cname" length="255"/>
		<property name="ccontent" column="ccontent" length="255"/>
		<property name="duration" column="duration" length="5"/>
	</class>
</hibernate-mapping>